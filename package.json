{
  "name": "cubby",
  "version": "0.2.0",
  "description": "Simple synchronous JSON storage for Node with TypeScript types.",
  "keywords": [
    "storage",
    "node",
    "json",
    "typescript"
  ],
  "author": "Chad Scira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/icodeforlove/node-cubby.git"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "test:types": "tsc -p tests/tsconfig.json --noEmit",
    "test": "npm run build && npm run test:types && node --test \"tests/**/*.js\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "typescript": "^5.6.0"
  },
  "dependencies": {}
}
